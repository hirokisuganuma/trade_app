<table border="1">
  <tr>
    <th>1~50h</th>
    <th>51~100</th>
    <th>101~150</th>
    <th>151~200</th>
  </tr>
  <tr>
    <td id="copyTarget-1">
      <% @ranking.slice(0, 50).each do |rank| %>
        <%= rank %><br>
      <% end %>
    </td>
    <td id="copyTarget-2">
      <% @ranking.slice(50, 50).each do |rank| %>
        <%= rank %><br>
      <% end %>
    </td>
    <td id="copyTarget-3">
      <% @ranking.slice(100, 50).each do |rank| %>
        <%= rank %><br>
      <% end %>
    </td>
    <td id="copyTarget-4">
      <% @ranking.slice(150, 50).each do |rank| %>
        <%= rank %><br>
      <% end %>
    </td>
  </tr>
</table>
